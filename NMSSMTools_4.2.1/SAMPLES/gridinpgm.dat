#	628	0d0		# KAPPA_max at MSUSY (If =/= min and XIF is not an input)
	637	-50d0		# ALAMBDA_min at MMESS (If =/= 0)
#	638	-50d0		# ALAMBDA_max at MMESS (If =/= min)
