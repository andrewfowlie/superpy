	#	#print JOB_FILE "         echo \$!\n";
	}
	print JOB_FILE "err=\$?\n";
	print JOB_FILE "if(test \$err -eq 123) then\n";
	print JOB_FILE "\techo Done.\n";
	print JOB_FILE "elif(test \$err -ne 0) then\n";
	print JOB_FILE "\techo CH Error \$err\n";
	print JOB_FILE "fi\n";
