			#		$run_begin[$l]+$run_step[$l]*$step_tmp[$l];
			#}
			my $total_cs=0;
			for (my $l=0;$l<=$#subprocesses;$l++){$total_cs = $total_cs + $cs[$l][$j];}
			$nevents[$k][$j] = int($n_events*$cs[$k][$j]/$total_cs);
			if($nevents[$k][$j]<10){$nevents[$k][$j]=10;}
